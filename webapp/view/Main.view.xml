<mvc:View controllerName="at.clouddna.fullscreennav.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="pageMain" title="{i18n>title}">
        <content>
            <List id="carrierList" headerText="Carrier" items="{/CarrierCollection}">
                <StandardListItem id="listItemTemplate" title="{CARRNAME}" description="{carrid}" icon="/img/{carrid}.png"
                    type="Navigation" press="onNavToDetail"/>
            </List>
        </content>
    </Page>
</mvc:View>
